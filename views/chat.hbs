 <div class="container" >
    <h2 class="center orange-text">{{roomName}}</h2>
    <ul class="collection" id="chat">
        {{#each msgs}}
    <li class="collection-item avatar #fffde7 yellow lighten-5">
        <i class="material-icons circle"> 
             <img src="{{personImageUrl}}" width="40px" height="40px" >
        </i>
        <span class="title"><strong>{{personName}}</strong></span>
        <p class="black-text">{{message}}</p>
        <a  class="secondary-content">
          {{!-- <i class="material-icons orange-text">mood</i> --}}
          <span class="orange-text">{{Date}}</span>
        </a>
    </li>
      
        {{/each}}
        {{!-- {{#each Dates}} --}}
          <span>{{Date}}</span>
        {{!-- {{/each}} --}}

      {{!-- <li class="collection-item">Mario</li> --}}
      
    </ul>
    <input type='hidden' id="personName" type="text" placeholder="Handle" name="handle" value="{{person.name}}" />
    <input type='hidden' id="personImageUrl" type="text" placeholder="Handle" name="handle" value="{{person.imageUrl}}" />
    <div class="row orange-text" >
      <div class="input-field col s12 orange-text">
          <i class="material-icons prefix orange-text" id="send">send</i>
        {{!-- <textarea id="textarea1" class="materialize-textarea orange-belt"></textarea>
        <label for="textarea1" class="orange-text">Message :</label> --}}
         <input id="textarea1" type="text" class="orange-belt" name="name">
         <label for="textarea1"  class="orange-text"> Message :</label>
      </div>
       <div class="row orange-text" >
      <div class="input-field col s12 orange-text">
       <input type="file" id="image" >
       <button type="submit" id="sendImage" > Send </button>
      </div>
    </div>
  </div>
    <script> const roomName = "{{roomName}}" 
</script>
  <!-- Compiled and minified JavaScript -->
  <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>
  <script src="/chat.js">    
  </script>